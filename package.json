{
  "name": "faker-community-discord-bot",
  "private": true,
  "type": "module",
  "scripts": {
    "build:clean": "rimraf dist",
    "start": "node --env-file .env --loader ts-node/esm src/index.ts",
    "build": "tsc --build tsconfig.build.json",
    "build:dev": "tsc --build tsconfig.json",
    "test:unit": "dotenv -e .env.test -- vitest .unit.test.ts --threads false",
    "test:integration": "dotenv -e .env.test -- vitest .integration.test.ts --threads false",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "^3.5.0",
    "@sinclair/typebox": "^0.31.28",
    "discord.js": "^14.14.1",
    "fastify": "^4.24.3",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.19.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.1.0"
  },
  "engines": {
    "node": "=20.9.0",
    "npm": "=10.1.0"
  }
}
